{
  "name": "YAPM by Eeren",
  "description": "Yet Another Password-Manager",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "https://www.googleapis.com/*"
  ],
  "icons": {"128": "icon128.png"}
}
